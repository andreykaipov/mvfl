
integer : /[0-9]+/
        ;
float   : /[0-9]+\.[0-9]+/
        ;
number  : <float>
        | <integer>
        ;

prefixOperator : "add" | "sub" | "mul" | "div"
               ;

expression : <factor> ( ('+'|'-') <factor> )*
           ;

factor : <term> ( ('*'|'/'|'%') <term> )*
       ;

term : <base> ('^' <term>)?
     | ('+'|'-') <term>
     ;

base : <number>
     | '(' <expression> ')'
     ;

mvfl : /^/ <expression> /$/ ;


