
integer : /[0-9]+/ ;
float   : /[0-9]+\.[0-9]+/ ;
number  : ('+'|'-')+ <number>
        | <float>
        | <integer> ;

prefixOperator : "add" | "sub" | "mul" | "div" ;

expression : <factor> ('+'|'-') <expression>
           | <factor> ;

factor : <term> ('*'|'/'|'%') <factor>
       | <term> ;

term : <base> '^' <term>
     | <base> ;

base : <number> | '(' <expression> ')' ;

mvfl : /^/ <expression> /$/ ;


